    <Version Condition="'$(Version)' == ''">0.0.1.0-alpha</Version>
    <Authors>yotphiligan</Authors>
    <Company>yotphiligan</Company>
    <Product>TrueMoon</Product>
    <PackageTags>Desktop</PackageTags>
    <Copyright>Copyright Â© yotphiligan 2022</Copyright>

    <LangVersion>latest</LangVersion>
    <DebugType>embedded</DebugType>
